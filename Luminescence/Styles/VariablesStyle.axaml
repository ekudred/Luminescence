<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style>
        <Style.Resources>
            <!-- Border -->
            <Color x:Key="BorderBrush">#d0d0d9</Color>
            <Thickness x:Key="BorderThickness">1</Thickness>
            <CornerRadius x:Key="CornerRadius">4</CornerRadius>
            
            <!-- Window -->
            <Color x:Key="WindowBackground">#ffffff</Color>
            
            <!-- TextBox, AccessText -->
            <Color x:Key="Foreground">#333333</Color>
            
            <!-- Any -->
            <SolidColorBrush x:Key="MenuFlyoutPresenterBackground">#ffffff</SolidColorBrush>
            
            <!-- Common -->
            <Color x:Key="ControlBackgroundDefault">#f7f7f7</Color>
            <Color x:Key="ControlBackgroundPointerover">#f7f7f7</Color>
            <Color x:Key="ControlBackgroundPressed">#f0f3f4</Color>
            <Color x:Key="ControlBackgroundSelected">#f0f3f4</Color>
            <Color x:Key="ControlBackgroundDisabled">Transparent</Color>
            <Color x:Key="ControlBackgroundFocus">#f7f7f7</Color>
            
        </Style.Resources>
    </Style>
</Styles>