<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia">
    
    <Style Selector="avalonia|MaterialIcon.Play">
        <Setter Property="Foreground" Value="#22b937" />
    </Style>

    <Style Selector="avalonia|MaterialIcon.Connection">
        <Setter Property="Foreground" Value="#22b937" />
    </Style>

    <Style Selector="avalonia|MaterialIcon.Stop">
        <Setter Property="Foreground" Value="#c72525" />
    </Style>

    <Style Selector="avalonia|MaterialIcon.WarningCircle">
        <Setter Property="RenderTransform">
            <ScaleTransform ScaleX="1" ScaleY="1" />
        </Setter>
        <Setter Property="Foreground" Value="#000000" />
    </Style>
    
</Styles>