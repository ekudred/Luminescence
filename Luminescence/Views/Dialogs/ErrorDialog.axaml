<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:Luminescence.ViewModels"
        mc:Ignorable="d"
        x:Class="Luminescence.Views.ErrorDialog"
        x:DataType="vm:ErrorDialogViewModel"
        x:CompileBindings="True"
        WindowStartupLocation="CenterScreen"
        WindowState="Normal"
        Width="500" Height="200"
        CanResize="False"
        ShowInTaskbar="False"
        SystemDecorations="Full"
        Icon="/Assets/avalonia-logo.ico"
        Title="Error | TlOsl">

    <Design.DataContext>
        <vm:ErrorDialogViewModel />
    </Design.DataContext>

    <!-- <Window.Styles> -->
    <!--     <StyleInclude Source="avares://Luminescence/Styles/Views/Dialogs/OptionsDialogStyle.axaml" /> -->
    <!-- </Window.Styles> -->

    <TextBlock Text="{Binding Text}" />

</Window>